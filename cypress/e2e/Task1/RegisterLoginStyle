describe('Verify Register/Login style form', () => {
  beforeEach(() => {
    
    cy.visit('https://victorious-wave-093b83610.1.azurestaticapps.net/#/login')
  })

  it('LogIn with valid data for "Admin" user',()=>{
    cy.get('button')
       .contains('Login')
       .should('be.visible')

    cy.get('a')  
      .contains('Reset') 
      .should('be.visible')

    cy.get('.sc-kmIPcE htFMXJ')  
      .contains('text','Login to')
      .should('be.visible')

    
  })


})